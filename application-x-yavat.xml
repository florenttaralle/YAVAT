<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-yavat">
    <comment>YAVAT Annotation File</comment>
    <glob pattern="*.yavat"/>
    <glob pattern="*.yvt"/>
    <icon name="x-yavat"/>
  </mime-type>
  <mime-type type="application/x-yavat-template">
    <comment>YAVAT Template File</comment>
    <glob pattern="*.yavat_template"/>
    <glob pattern="*.yvtt"/>
    <icon name="x-yavat-template"/>
  </mime-type>
</mime-info>
